
<div class="bellow-navbar">
  <div class="text-center">
    <h2>My places on AirBnB</h2>
  </div>

  <div class="container">
    <div class="row">
       <% @products.each do |product| %>
       <%= link_to(product) do %>
         <div class="col-xs-6 col-sm-4">
           <%= render "products/card", product: product, footer: nil %>
         </div>
      <% end %>
      <% end %>
        </div>
    </div>
  </div>
</div>
